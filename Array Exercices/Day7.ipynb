    "#Problem7 : Plus One\n",
    "class Solution:\n",
    "    def plusOne(self, digits: List[int]) -> List[int]:\n",
    "        newarr = int(''.join(map(str,digits)))\n",
    "        newarr = newarr  + 1 \n",
    "        finalarr = [ ]\n",
    "        for num in str(newarr):\n",
    "            finalarr.append(int(num))\n",
    "        return finalarr"
   ]
